{
  "name": "e-commerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "next test",
    "lint": "eslint --ext \".ts,.js,.tsx\" --ignore-path .eslintignore .",
    "postinstall": "husky install",
    "lintfix": "eslint --fix --ext \".ts,.js,.jsx\" --ignore-path .eslintignore .",
    "prepare": "husky install",
    "check-types": "tsc --noEmit --pretty",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "yarn check-types && yarn check-format && yarn check-lint && yarn build",
    "clean": "rm -rf .nuxt dist .output"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@mantine/carousel": "^6.0.1",
    "@mantine/core": "^6.0.1",
    "@mantine/form": "^6.0.5",
    "@mantine/hooks": "^6.0.1",
    "@mantine/next": "^6.0.1",
    "@types/node": "18.15.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "^10.4.14",
    "embla-carousel-react": "^7.1.0",
    "eslint": "^8.0.1",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "postcss": "^8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "typescript": "*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,html}": [
      "yarn lintfix"
    ]
  }
}
